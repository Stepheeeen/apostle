{
  "expo": {
    "name": "apostle",
    "slug": "apostle",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Apostle-Logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Apostle-Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.washwisee.apostle"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Apostle-Logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.washwisee.apostle"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Apostle-Logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "1768bbb5-656c-4a5b-8967-eca6e8bef51e"
      }
    }
  }
}
